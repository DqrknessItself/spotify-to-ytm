{
  "manifest_version": 2,
  "name": "spotify-to-ytm",
  "version": "1.0",

  "description": "Redirects web links from Spotify to YouTube music",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.spotify.com/*", "*://music.youtube.com/*"],
      "js": ["test.js"]
    }
  ]
}