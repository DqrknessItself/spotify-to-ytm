{
  "manifest_version": 2,
  "name": "Redirect: Spotify -> YouTube Music",
  "version": "1.0",

  "description": "Redirects web links from Spotify to YouTube music",

  "icons": {
    "48": "icons/border-48.png",
    "96": "icons/border-96.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.spotify.com/*", "*://music.youtube.com/*"],
      "js": ["redirect.js"]
    }
  ]
}